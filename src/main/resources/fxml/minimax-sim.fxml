                    </graphic>
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0"/>
                    </content>
                </Tab>
                <Tab fx:id="tab_reg" text="Register-Erweiterungen">
                    <graphic>
                        <ImageView>
                            <Image url="/images/fugue/database.png"/>
                        </ImageView>
                    </graphic>
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0"/>
                    </content>
                </Tab>
                <Tab fx:id="tab_mux" text="Multiplexer-Anschlüsse">
                    <graphic>
                        <ImageView>
                            <Image url="/images/fugue/node-select-all2.png"/>
                        </ImageView>
                    </graphic>
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0"/>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </children>
</GridPane>
